source 'https://rubygems.org'
gemspec

# To Read xlsx files
gem 'roo'

# To read/write xlsx files
gem 'rubyXL'

# To write fast in XLSX
gem 'xlsxtream'

# To read xlsx file faster
gem 'creek'

# To handle MongoDB database
gem 'mongo'

# To get awesome print
gem 'awesome_print'

# Simple progress bar
gem 'progress_bar'

# source "http://rubygems.org"

# iruby dependencies
gem 'rbczmq'
gem 'ffi-rzmq'
gem 'iruby'

# fetch from the github master branch
